<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="data"
            type="com.king.mvvmframe.data.model.OilPrice"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:background="?android:attr/selectableItemBackground">
        <TextView
            android:id="@+id/tvCity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_3"
            android:textSize="18sp"
            android:layout_margin="2dp"
            android:layout_alignParentEnd="true"
            android:text="@{data.city}"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="北京"/>
        <TextView
            android:id="@+id/tvLabel92h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_6"
            android:textSize="16sp"
            android:maxLines="1"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:text="@string/oil_92h_"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvCity"
            app:layout_constraintRight_toLeftOf="@+id/tv92h"/>
        <TextView
            android:id="@+id/tv92h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_oil_price"
            android:textSize="18sp"
            android:maxLines="1"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:text="@{data.oil92Str}"
            app:layout_constraintLeft_toRightOf="@+id/tvLabel92h"
            app:layout_constraintTop_toTopOf="@+id/tvLabel92h"
            app:layout_constraintBottom_toBottomOf="@+id/tvLabel92h"
            app:layout_constraintRight_toLeftOf="@+id/v1"
            tools:text="¥ 6.99"/>
        <View
            android:id="@+id/v1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/tvLabel92h"
            app:layout_constraintBottom_toBottomOf="@id/tvLabel92h"
            app:layout_constraintLeft_toRightOf="@+id/tv92h"
            app:layout_constraintRight_toLeftOf="@+id/tvLabel95h"/>
        <TextView
            android:id="@+id/tvLabel95h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_6"
            android:textSize="16sp"
            android:maxLines="1"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:text="@string/oil_95h_"
            app:layout_constraintLeft_toRightOf="@+id/v1"
            app:layout_constraintTop_toTopOf="@+id/tvLabel92h"
            app:layout_constraintBottom_toBottomOf="@+id/tvLabel92h"
            app:layout_constraintRight_toLeftOf="@+id/tv95h" />

        <TextView
            android:id="@+id/tv95h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_oil_price"
            android:textSize="18sp"
            android:maxLines="1"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:text="@{data.oil95Str}"
            app:layout_constraintLeft_toRightOf="@+id/tvLabel95h"
            app:layout_constraintTop_toTopOf="@+id/tvLabel92h"
            app:layout_constraintBottom_toBottomOf="@+id/tvLabel92h"
            app:layout_constraintRight_toRightOf="parent"
            tools:text="¥ 6.99"/>
        <TextView
            android:id="@+id/tvLabel98h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_6"
            android:textSize="16sp"
            android:maxLines="1"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:text="@string/oil_98h_"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvLabel92h"
            app:layout_constraintRight_toLeftOf="@+id/tv98h"/>
        <TextView
            android:id="@+id/tv98h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_oil_price"
            android:textSize="18sp"
            android:maxLines="1"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:text="@{data.oil98Str}"
            app:layout_constraintLeft_toRightOf="@+id/tvLabel98h"
            app:layout_constraintTop_toTopOf="@+id/tvLabel98h"
            app:layout_constraintBottom_toBottomOf="@+id/tvLabel98h"
            app:layout_constraintRight_toLeftOf="@+id/v2"
            tools:text="¥ 6.99"/>
        <View
            android:id="@+id/v2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/tvLabel98h"
            app:layout_constraintBottom_toBottomOf="@id/tvLabel98h"
            app:layout_constraintLeft_toRightOf="@+id/tv98h"
            app:layout_constraintRight_toLeftOf="@+id/tvLabel0h"/>
        <TextView
            android:id="@+id/tvLabel0h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_6"
            android:textSize="16sp"
            android:maxLines="1"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:text="@string/oil_0h_"
            app:layout_constraintLeft_toRightOf="@+id/v2"
            app:layout_constraintTop_toTopOf="@+id/tvLabel98h"
            app:layout_constraintBottom_toBottomOf="@+id/tvLabel98h"
            app:layout_constraintRight_toLeftOf="@+id/tv95h" />

        <TextView
            android:id="@+id/tv0h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_oil_price"
            android:textSize="18sp"
            android:maxLines="1"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:text="@{data.oil0Str}"
            app:layout_constraintLeft_toRightOf="@+id/tvLabel0h"
            app:layout_constraintTop_toTopOf="@+id/tvLabel98h"
            app:layout_constraintBottom_toBottomOf="@+id/tvLabel98h"
            app:layout_constraintRight_toRightOf="parent"
            tools:text="¥ 6.99"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
